        <span class="bp">self</span><span class="p">.</span><span class="n">classes</span><span class="p">.</span><span class="n">append</span><span class="p">(</span><span class="n">class_name</span><span class="p">)</span>  
  
<span class="k">class</span> <span class="nc">Master</span><span class="p">(</span><span class="n">Student</span><span class="p">):</span> <span class="c1"># Student ìƒì†  
</span>	<span class="k">def</span> <span class="nf">__init__</span><span class="p">(</span><span class="bp">self</span><span class="p">,</span> <span class="n">name</span><span class="p">:</span> <span class="nb">str</span><span class="p">,</span> <span class="n">sid</span><span class="p">:</span> <span class="nb">int</span><span class="p">,</span> <span class="n">professor</span><span class="p">:</span> <span class="nb">str</span><span class="p">):</span>  
