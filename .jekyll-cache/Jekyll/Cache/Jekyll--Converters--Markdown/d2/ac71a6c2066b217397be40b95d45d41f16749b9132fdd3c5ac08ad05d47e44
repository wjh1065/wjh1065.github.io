	<span class="c1"># í´ë˜ìŠ¤ í•¨ìˆ˜ (Method)
</span>	<span class="k">def</span> <span class="nf">take_class</span><span class="p">(</span><span class="bp">self</span><span class="p">,</span> <span class="n">class_name</span><span class="p">:</span> <span class="nb">str</span><span class="p">)</span> <span class="o">-&gt;</span> <span class="bp">None</span><span class="p">:</span>  
