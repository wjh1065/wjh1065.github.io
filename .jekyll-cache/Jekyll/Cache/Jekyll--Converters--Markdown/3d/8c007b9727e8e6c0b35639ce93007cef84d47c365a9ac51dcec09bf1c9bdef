<span class="k">assert</span> <span class="nb">all</span><span class="p">(</span><span class="n">test2</span><span class="p">)</span> <span class="o">==</span> <span class="n">my_all</span><span class="p">(</span><span class="n">test2</span><span class="p">)</span> <span class="o">==</span> <span class="bp">True</span>

